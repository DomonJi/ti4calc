{
  "name": "ti4-calc",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "format": "prettier --write \"{,!(.next|dist|node_modules)/**/}!(package-lock).{json,md,js,jsx,ts,tsx,css,scss}\"",
    "lint": "node ./node_modules/eslint/bin/eslint.js --ext .ts --ext .tsx --fix .",
    "pre-commit": "lint-staged",
    "pre-push": "npm run lint && cross-env CI=true npm run test && npm run typecheck",
    "start": "next start -p 8200",
    "test": "jest",
    "typecheck": "tsc --noEmit",
    "prepare": "husky install"
  },
  "dependencies": {
    "@types/lodash": "4.14.177",
    "@types/pg": "8.6.1",
    "@types/react": "17.0.35",
    "@types/styled-components": "5.1.15",
    "cross-env": "7.0.3",
    "husky": "7.0.4",
    "lodash": "4.17.21",
    "next": "12.0.4",
    "pg": "8.7.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "styled-components": "5.3.3",
    "typescript": "4.5.2",
    "worker-loader": "3.0.8"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "12.0.4",
    "@types/jest": "27.0.2",
    "@typescript-eslint/eslint-plugin": "5.4.0",
    "@typescript-eslint/parser": "5.4.0",
    "babel-jest": "27.3.1",
    "babel-plugin-styled-components": "1.13.3",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-react": "7.27.0",
    "eslint-plugin-react-hooks": "4.3.0",
    "jest": "27.3.1",
    "jest-watch-typeahead": "1.0.0",
    "lint-staged": "12.0.2",
    "prettier": "2.4.1"
  },
  "lint-staged": {
    "*.{json,md,css,scss}": [
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "node ./node_modules/eslint/bin/eslint.js --ext .ts --ext .tsx --fix",
      "prettier --write"
    ]
  }
}
