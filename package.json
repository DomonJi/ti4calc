{
  "name": "ti4-calc",
  "version": "0.1.0",
  "private": false,
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "dev-database": "docker compose -f docker-compose-dev.yml up -d",
    "dev-database-build": "docker compose -f docker-compose-dev.yml build",
    "docker-build": "docker compose build --no-cache",
    "docker-up": "docker compose up",
    "format": "prettier --write \"{,!(.next|dist|node_modules)/**/}!(package-lock).{json,md,js,jsx,ts,tsx,css,scss}\"",
    "lint": "eslint --ext .ts --ext .tsx --fix .",
    "pre-commit": "lint-staged",
    "pre-push": "npm run lint && cross-env CI=true npm run test && npm run typecheck",
    "start": "next start -p 3000",
    "test": "jest",
    "typecheck": "tsc --noEmit",
    "prepare": "husky install"
  },
  "dependencies": {
    "@types/lodash": "4.14.199",
    "@types/pg": "8.10.5",
    "@types/react": "18.2.28",
    "cross-env": "7.0.3",
    "husky": "8.0.3",
    "lodash": "4.17.21",
    "next": "13.5.4",
    "pg": "8.11.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "typescript": "5.2.2"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "13.5.4",
    "@types/jest": "29.5.5",
    "@typescript-eslint/eslint-plugin": "6.7.5",
    "@typescript-eslint/parser": "6.7.5",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-no-only-tests": "3.1.0",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "lint-staged": "14.0.1",
    "prettier": "3.0.3",
    "sass": "1.69.3"
  },
  "lint-staged": {
    "*.{json,md,css,scss,html}": [
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "eslint --ext .ts --ext .tsx --fix",
      "prettier --write"
    ]
  }
}
